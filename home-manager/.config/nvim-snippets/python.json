{
    "if_name_main": {
        "prefix": ["name"],
        "body": [
            "if __name__ == '__main__':",
            "\t$0"
        ]
    },
	"headblock": {
        "prefix": "headblock",
        "body": [
            "########################################",
            "# $1",
            "# keywords  | $2",
            "########################################",
            "$0"
        ]
    },
    "no-arg-type-pyright": {
        "prefix": "no-arg-type-pyright",
        "body": [
            "# pyright: reportArgumentType=false"
        ]
    }
}
